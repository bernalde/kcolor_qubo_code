$include cycle.inc
*$include devil5.inc
*$include devil50.inc
*$include devil500.inc

set r /1*3/;
alias(r,rr);

binary variable x(i,r);

variable alpha;

equation objective;

parameter
C1 /2/,
C2 /2/
;

objective.. sum(i,x(i)*x(i)) - M*sum(E(i,j),x(i)*x(j)) =E= alpha;

model stableset /all/;

option miqcp = gurobi;

solve stableset using miqcp max alpha;

display alpha.l;
